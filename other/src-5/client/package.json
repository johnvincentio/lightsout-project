		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-react": "^7.0.0",
		"acorn": "^6.0.7",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^23.6.0",
		"babel-loader": "^8.0.5",
		"clean-webpack-plugin": "^1.0.1",
		"copy-webpack-plugin": "^4.6.0",
